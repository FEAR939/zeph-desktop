{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "all": true,
      "allow": [
        "http://*",
        "https://*",
        "http://*:80/*",
        "https://*:5000/*"
      ]
    },
    "core:window:allow-set-fullscreen",
    "shell:default",
    "http:default",
    "http:default"
  ]
}