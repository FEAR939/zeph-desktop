{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": ["http://*", "https://*", "http://localhost:5000/*"]
    },
    "core:window:allow-set-fullscreen",
    "shell:default"
  ]
}
